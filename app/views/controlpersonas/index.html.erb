  </tr>

<% @controlpersonas.each do |controlpersona| %>
  <tr>
    <td><%= controlpersona.numerodocumento %></td>
    <td><%= controlpersona.estado %></td>
    <td><%= link_to 'Show', controlpersona %></td>
